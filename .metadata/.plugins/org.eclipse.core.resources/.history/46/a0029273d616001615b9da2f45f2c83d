package CoachSim;

public class BaseballStat {
	
	private String hit;
	private int bases;
	
	public BaseballStat(){
		this(" ", 0);
	}
	public BaseballStat(String hit, int bases) {
		super();
		this.hit = hit;
		this.bases = bases;
	}
	
	public String getHit(){
		return hit;
	}
	
	public int getBases(){
		
		return bases;
	}
	
	
}
